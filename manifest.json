{
  "name": "MOODLE Quiz Solver",
  "description": "This extension allows you to master the hardest MOODLE exams.",
  "version": "0.2.0",
  "permissions": ["storage", "declarativeContent", "tabs", "*://*/"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["./src/scripts/background.js"],
    "persistent": false
  },
  "icons": {
    "16": "./src/images/pic16.png",
    "32": "./src/images/pic32.png",
    "48": "./src/images/pic48.png",
    "128": "./src/images/pic128.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-buwfl/YYIyq2hjhtEwKblSHoC4xTVlLHuXON6J7lG2g='; object-src 'self';",
  "manifest_version": 2
}
